<h3 class="q3">Question {{@index}}</h3>
<figure>
    <figcaption>Title</figcaption>
    <ul>
        <li>{{this.title}}</li>
    </ul>
</figure>
<figure>
    <figcaption>Max points</figcaption>
    <ul>
        <li>{{this.maxPoints}}</li>
    </ul>
</figure>
<figure>
    <figcaption>Options</figcaption>
    <ul>
        {{#each options}}
        {{>option}}
        {{/each}}
    </ul>
</figure>